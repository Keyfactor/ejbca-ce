<suite name="SCEPTest" verbose="2">
	<test name="SCEP test: Start with just 3 calls since it may fail if started with a heavy load." >
		<parameter name="commandLine" value="SCEPTest http://ca:8080/ejbca/publicweb/apply/scep/noca/pkiclient.exe SCEPCA 1:3"/>
		<parameter name="statusCode" value="nostatuscheck"/>
	</test>
	<test name="SCEP stress test." >
		<parameter name="commandLine" value="SCEPTest http://ca:8080/ejbca/publicweb/apply/scep/noca/pkiclient.exe SCEPCA 10:25 1000"/>
		<parameter name="statusCode" value="0"/>
	</test>
</suite>
