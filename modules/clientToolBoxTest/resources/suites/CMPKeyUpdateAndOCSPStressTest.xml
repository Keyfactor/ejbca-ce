<suite name="CMPKeyUpdateStressTest and OCSP stress" verbose="2">
	<test name="CMP client stress." >
		<parameter name="caName" value="CMPCA"/>
		<parameter name="password" value="foo123"/>
		<parameter name="keyDir" value="keys"/>
		<parameter name="nr" value="10"/>
		<parameter name="deleteFileBefore" value="result.log"/>
		<parameter name="commandLine" value="CMPKeyUpdateStressTest ca keys foo123 testClient 10:100 1000 8080"/>
		<parameter name="statusCode" value="0"/>
		<classes>
			<class name="org.ejbca.ui.cli.clientToolBoxTest.tests.P12" />
		</classes>
	</test>
	<test name="OCSP stress with client signing." >
		<parameter name="commandLine" value="OCSP stress http://ca:8080/ejbca/publicweb/status/ocsp result.log CMPCA.cacert.pem 30:100 1000 GET keys/tester-0.p12 foo123"/>
		<parameter name="statusCode" value="0"/>
	</test>
</suite>
