    <tr  id="Row<%=row++%2%>"> 
      <td width="40%"  align="right"> 
        <%= ejbcawebbean.getText("NUMBEROFCOPIES") %> 
      </td>
      <td width="60%">
        <select name="<%=EditHardTokenProfileJSPHelper.SELECT_NUMOFTOKENCOPIES%>" size="1"  >
