<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<%               
  HardTokenProfile profiledata = edithardtokenprofile.profiledata;
   

  Map authorizedcas = ejbcawebbean.getInformationMemory().getCANames();
