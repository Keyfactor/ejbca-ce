<faces-config version="2.2"
    metadata-complete="false"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                        http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

  <navigation-rule>
  	<from-view-id>/approval/approvalaction.xhtml</from-view-id>
  	<navigation-case>
  		<from-outcome>approveaction</from-outcome>
  		<to-view-id>/approval/approvalaction.xhtml</to-view-id>
  	</navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/ca/caactivation.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>apply</from-outcome>
      <to-view-id>/ca/caactivation.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>

  <navigation-rule>
  	<from-view-id>/initpki.xhtml</from-view-id>
  	<navigation-case>
  		<from-outcome>newPkiMode</from-outcome>
  		<to-view-id>/initnewpki.xhtml</to-view-id>
  	</navigation-case>
	<navigation-case>
  		<from-outcome>existingPkiMode</from-outcome>
  		<to-view-id>/initexistingpki.xhtml</to-view-id>
  	</navigation-case>
  	<navigation-case>
  		<from-outcome>skip</from-outcome>
  		<to-view-id>/ca/editcas/managecas.xhtml</to-view-id>
  	</navigation-case>
  </navigation-rule>
  
  <navigation-rule>
  	<from-view-id>/initnewpki.xhtml</from-view-id>
  	<navigation-case>
  		<from-outcome>back</from-outcome>
  		<to-view-id>/initpki.xhtml</to-view-id>
  	</navigation-case>
 	<navigation-case>
  		<from-outcome>createNewToken</from-outcome>
  		<to-view-id>/cryptotoken/cryptotoken.xhtml</to-view-id>
      	<redirect include-view-params="true"></redirect>
  	</navigation-case>
  	<navigation-case>
  		<from-outcome>next</from-outcome>
  		<to-view-id>/initpkiadmin.xhtml</to-view-id>
  	</navigation-case>
  </navigation-rule>
  
  <navigation-rule>
  	<from-view-id>/initpkiadmin.xhtml</from-view-id>
  	<navigation-case>
  		<from-outcome>back</from-outcome>
  		<to-view-id>/initnewpki.xhtml</to-view-id>
  	</navigation-case>
 	<navigation-case>
  		<from-outcome>next</from-outcome>
  		<to-view-id>/initpkisummary.xhtml</to-view-id>
  	</navigation-case>
  </navigation-rule>
  
  <navigation-rule>
  	<from-view-id>/initpkisummary.xhtml</from-view-id>
  	<navigation-case>
  		<from-outcome>back</from-outcome>
  		<to-view-id>/initpkiadmin.xhtml</to-view-id>
  	</navigation-case>
  </navigation-rule>
  
  <navigation-rule>
  	<from-view-id>/cryptotoken/cryptotoken.xhtml</from-view-id>
  	<navigation-case>
  		<from-outcome>back</from-outcome>
  		<to-view-id>/initnewpki.xhtml</to-view-id>
  	</navigation-case>
  </navigation-rule>
  
  <navigation-rule>
  	<from-view-id>/initexistingpki.xhtml</from-view-id>
  	<navigation-case>
  		<from-outcome>back</from-outcome>
  		<to-view-id>/initpki.xhtml</to-view-id>
  	</navigation-case>
 	<navigation-case>
  		<from-outcome>next</from-outcome>
  		<to-view-id>/initexistingpkirole.xhtml</to-view-id>
  	</navigation-case>
  </navigation-rule>
  
  <navigation-rule>
  	<from-view-id>/initexistingpkirole.xhtml</from-view-id>
  	<navigation-case>
  		<from-outcome>back</from-outcome>
  		<to-view-id>/initexistingpki.xhtml</to-view-id>
  	</navigation-case>
  	<navigation-case>
  		<from-outcome>next</from-outcome>
  		<to-view-id>/administratorprivileges/roles.xhtml</to-view-id>
  	</navigation-case>
  </navigation-rule>

  <navigation-rule>
  	<from-view-id>/sysconfig/cmpaliases.xhtml</from-view-id>
  	<navigation-case>
  		<from-outcome>edit</from-outcome>
  		<to-view-id>/sysconfig/cmpalias.xhtml</to-view-id>
  	</navigation-case>
  </navigation-rule>
  <navigation-rule>
  	<from-view-id>/sysconfig/cmpaliases.xhtml</from-view-id>
  	<navigation-case>
  		<from-outcome>view</from-outcome>
  		<to-view-id>/sysconfig/cmpalias.xhtml</to-view-id>
  	</navigation-case>
  </navigation-rule>
   <navigation-rule>
  	<from-view-id>/sysconfig/cmpalias.xhtml</from-view-id>
  	<navigation-case>
  		<from-outcome>done</from-outcome>
  		<to-view-id>/sysconfig/cmpaliases.xhtml</to-view-id>
  	</navigation-case>
  </navigation-rule>

    <navigation-rule>
        <from-view-id>/sysconfig/autoenrollconfiguration.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/sysconfig/editautoenrollmentconfiguration.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/sysconfig/autoenrollconfiguration.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>view</from-outcome>
            <to-view-id>/sysconfig/editautoenrollmentconfiguration.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/sysconfig/editautoenrollmentconfiguration.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>done</from-outcome>
            <to-view-id>/sysconfig/autoenrollconfiguration.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/sysconfig/estconfigurations.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/sysconfig/editestalias.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/sysconfig/estconfigurations.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>view</from-outcome>
            <to-view-id>/sysconfig/editestalias.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/sysconfig/editestalias.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>done</from-outcome>
            <to-view-id>/sysconfig/estconfigurations.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

  <navigation-rule>
    <from-view-id>/ca/editcertificateprofiles/editcertificateprofiles.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>edit</from-outcome>
      <to-view-id>/ca/editcertificateprofiles/editcertificateprofile.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>view</from-outcome>
      <to-view-id>/ca/editcertificateprofiles/editcertificateprofile.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/ca/editcertificateprofiles/editcertificateprofile.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>done</from-outcome>
      <to-view-id>/ca/editcertificateprofiles/editcertificateprofiles.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>

  <navigation-rule>
    <from-view-id>/ca/editvalidators/editvalidators.xhtml</from-view-id>
    <navigation-case>
      <from-action>#{validatorsBean.actionEdit}</from-action>
      <from-outcome>edit</from-outcome>
      <to-view-id>/ca/editvalidators/editvalidator.xhtml</to-view-id>
      <redirect include-view-params="true"></redirect>
    </navigation-case>
    <navigation-case>
      <from-action>#{validatorsBean.actionAddFromTemplate}</from-action>
      <from-outcome>clone</from-outcome>
      <to-view-id>/ca/editvalidators/editvalidator.xhtml</to-view-id>
      <redirect include-view-params="true"></redirect>
    </navigation-case>
    <navigation-case>
      <from-action>#{validatorsBean.actionAdd}</from-action>
      <from-outcome>add</from-outcome>
      <to-view-id>/ca/editvalidators/editvalidator.xhtml</to-view-id>
      <redirect include-view-params="true"></redirect>
    </navigation-case>
    <navigation-case>
      <from-outcome>view</from-outcome>
      <to-view-id>/ca/editvalidators/editvalidator.xhtml</to-view-id>
      <redirect include-view-params="true"></redirect>
    </navigation-case>
    <navigation-case>
      <from-action>#{validatorsBean.#{validatorsBean.actionRenameConfirm}}</from-action>
      <to-view-id>/ca/editvalidators/editvalidators.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-action>#{validatorsBean.#{validatorsBean.actionDeleteConfirm}}</from-action>
      <to-view-id>/ca/editvalidators/editvalidators.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-action>#{validatorsBean.#{validatorsBean.actionAddFromTemplateConfirm}}</from-action>
      <to-view-id>/ca/editvalidators/editvalidators.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/ca/editvalidators/editvalidator.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>done</from-outcome>
      <to-view-id>/ca/editvalidators/editvalidators.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>

   <navigation-rule>
    <from-view-id>/approval/editapprovalprofiles.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>edit</from-outcome>
      <to-view-id>/approval/editapprovalprofile.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>view</from-outcome>
      <to-view-id>/approval/editapprovalprofile.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/approval/editapprovalprofile.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>done</from-outcome>
      <to-view-id>/approval/editapprovalprofiles.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>

    <navigation-rule>
        <from-view-id>/ra/edituserdatasources/userdatasourcespage.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{userDatasoucesMBean.actionEdit}</from-action>
            <from-outcome>edit</from-outcome>
            <to-view-id>/ra/edituserdatasources/edituserdatasource.xhtml</to-view-id>
            <redirect include-view-params="true"></redirect>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/ra/edituserdatasources/edituserdatasource.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>done</from-outcome>
            <to-view-id>/ra/edituserdatasources/userdatasourcespage.xhtml</to-view-id>
        </navigation-case>
</navigation-rule>

  <navigation-rule>
    <from-view-id>/sysconfig/systemconfiguration.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>edit</from-outcome>
      <to-view-id>/sysconfig/customcertextension.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>view</from-outcome>
      <to-view-id>/sysconfig/customcertextension.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>editCTLog</from-outcome>
      <to-view-id>/sysconfig/editctlog.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>editOAuthKey</from-outcome>
      <to-view-id>/sysconfig/editoauthkey.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>

  <navigation-rule>
    <from-view-id>/sysconfig/editctlog.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>saved</from-outcome>
      <to-view-id>/sysconfig/systemconfiguration.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>

  <navigation-rule>
    <from-view-id>/sysconfig/editoauthkey.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>saved</from-outcome>
      <to-view-id>/sysconfig/systemconfiguration.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>

  <navigation-rule>
  	<from-view-id>/services/listservices.xhtml</from-view-id>
  	<navigation-case>
  		<from-outcome>listservices</from-outcome>
  		<to-view-id>/services/listservices.xhtml</to-view-id>
  	</navigation-case>
  	<navigation-case>
  		<from-outcome>editservice</from-outcome>
  		<to-view-id>/services/editservice.xhtml</to-view-id>
  	</navigation-case>
  </navigation-rule>
  <navigation-rule>
  	<from-view-id>/services/editservice.xhtml</from-view-id>
  	<navigation-case>
  		<from-outcome>listservices</from-outcome>
  		<to-view-id>/services/listservices.xhtml</to-view-id>
  	</navigation-case>
  	<navigation-case>
  		<from-outcome>editservice</from-outcome>
  		<to-view-id>/services/editservice.xhtml</to-view-id>
  	</navigation-case>
  </navigation-rule>

  <navigation-rule>
  	<from-view-id>/peerconnector/peerconnectors.xhtml</from-view-id>
  	<navigation-case>
  		<from-outcome>edit</from-outcome>
  		<to-view-id>/peerconnector/peerconnector.xhtml</to-view-id>
  	</navigation-case>
  	<navigation-case>
  		<from-outcome>done</from-outcome>
  		<to-view-id>/peerconnector/peerconnectors.xhtml</to-view-id>
  	</navigation-case>
  	<navigation-case>
  		<from-outcome>manage</from-outcome>
  		<to-view-id>/peerconnector/peermgmt.xhtml</to-view-id>
  	</navigation-case>
  	<navigation-case>
        <from-outcome>peerprocessauth</from-outcome>
	    <to-view-id>/peerconnector/peerprocessauth.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>peerinauth</from-outcome>
        <to-view-id>/peerconnector/peerinauth.xhtml</to-view-id>
	</navigation-case>
  </navigation-rule>

  <navigation-rule>
  	<from-view-id>/peerconnector/peerconnector.xhtml</from-view-id>
  	<navigation-case>
  		<from-outcome>done</from-outcome>
  		<to-view-id>/peerconnector/peerconnectors.xhtml</to-view-id>
  	</navigation-case>
  </navigation-rule>
  <navigation-rule>
  	<from-view-id>/peerconnector/peermgmt.xhtml</from-view-id>
  	<navigation-case>
  		<from-outcome>done</from-outcome>
  		<to-view-id>/peerconnector/peerconnectors.xhtml</to-view-id>
  	</navigation-case>
  </navigation-rule>

  <factory>
  		<exception-handler-factory>org.ejbca.ui.web.admin.CaExceptionHandlerFactory</exception-handler-factory>
  </factory>

</faces-config>
