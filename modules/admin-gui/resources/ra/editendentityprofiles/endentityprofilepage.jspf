function checkuseendentityemail(i) {
	var valuefield = eval("document.editprofile.<%= TEXTFIELD_SUBJECTALTNAME %>" + i);
	var checkbox = eval("document.editprofile.<%=CHECKBOX_USE_SUBJECTALTNAME %>" + i);
