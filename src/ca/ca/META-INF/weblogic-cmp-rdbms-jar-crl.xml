<?xml version="1.0"?>

<!DOCTYPE weblogic-rdbms-bean PUBLIC '-//BEA Systems, Inc.//DTD WebLogic 5.1.0 EJB RDBMS Persistence//EN' 'http://www.bea.com/servers/wls510/dtd/weblogic-rdbms-persistence.dtd'>


<weblogic-rdbms-bean>

  <pool-name>DefaultPool</pool-name>
  <table-name>CRLDATA</table-name>
  <attribute-map>
    <object-link>
       <bean-field>b64Crl</bean-field>
       <dbms-column>b64Crl</dbms-column>
    </object-link>
    <object-link>
       <bean-field>fp</bean-field>
       <dbms-column>fp</dbms-column>
    </object-link>
    <object-link>
      <bean-field>CRLNumber</bean-field>
      <dbms-column>CRLNumber</dbms-column>
    </object-link>
    <object-link>
      <bean-field>issuerDN</bean-field>
      <dbms-column>issuerDN</dbms-column>
    </object-link>
    <object-link>
      <bean-field>cafp</bean-field>
      <dbms-column>cafp</dbms-column>
    </object-link>
    <object-link>
      <bean-field>thisUpdate</bean-field>
      <dbms-column>thisUpdate</dbms-column>
    </object-link>
    <object-link>
      <bean-field>nextUpdate</bean-field>
      <dbms-column>nextUpdate</dbms-column>
    </object-link>
  </attribute-map>

</weblogic-rdbms-bean>


